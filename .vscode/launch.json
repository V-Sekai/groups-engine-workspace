{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Projects (fire)",
      "type": "lldb",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      // "args": ["perf record -o perf.data --call-graph dwarf --aio --sample-cpu ./godot/bin/godot.linuxbsd.opt.tools.64.llvm --path groups"],
      // "args": ["--path", "godot-modules-groups/ewbik/demo","-e"],
      "args": ["--path", "groups","-e"],
      "program": "${workspaceFolder}/godot/bin/godot.linuxbsd.opt.tools.64.llvm",
      "preLaunchTask": "Build Linux"
    },
    {
      "name": "Launch editor Macos",
      "type": "lldb",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "miDebuggerPath": "/usr/bin/gdb",
      "args": ["--path", "groups","-e"],
      "program": "${workspaceFolder}/godot/bin/godot.osx.opt.tools.64",
      "preLaunchTask": "Build osx"
    },
    {
      "name": "Launch regression test",
      "type": "lldb",
      "request": "launch",
      "cwd": "${workspaceFolder}",
      "miDebuggerPath": "/usr/bin/gdb",
      "args": ["--audio-driver", "Dummy", "--path", "regression-tests"],
      "program": "${workspaceFolder}/godot/bin/godot.linuxbsd.opt.tools.64.llvm",
      "visualizerFile": "${workspaceFolder}/godot/platform/windows/godot.natvis",
      "preLaunchTask": "Build Linux"
    },
  ]
}

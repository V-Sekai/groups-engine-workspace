{
  // See https://go.microsoft.com/fwlink/?LinkId=733558
  // for the documentation about the tasks.json format
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Build Windows",
      "type": "shell",
      "command": "scons -j8 platform=windows bits=64 target=release_debug custom_modules=../groups_godot_modules",
      "options": {
        "cwd": "${workspaceFolder}/godot"
      },
      "problemMatcher": [],
      "group": "build",
      "dependsOn": [
        "Update repos.sh"
      ]
    },
  ]
}

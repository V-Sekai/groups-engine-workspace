{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "type": "shell",
            "command": "scons platform=windows use_llvm=yes use_mingw=yes -j30 bits=64 deprecated=yes target=release_debug custom_modules=../groups_godot_modules",
            "options": {
                "cwd": "${workspaceFolder}/godot"
            },
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": []
        }
    ]
}